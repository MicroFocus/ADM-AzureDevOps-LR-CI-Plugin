{
    "manifestVersion": 1,
    "id": "LoadRunner-Tests-Plugin",
    "name": "LoadRunner tests automation",
    "version": "1.0.0",
    "publisher": "Micro-Focus-LR",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "For more details visit [official documentation](https://github.com/MicroFocus/ADM-AzureDevOps-LR-CI-Plugin/blob/master/README.md)",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/LoadRunner.png"        
    },
    "files": [
        {
            "path": "LoadRunnerTask"
        }
    ],
    "contributions": [
        {
            "id": "custom-build-release-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "LoadRunnerTask"
            }
        }
    ]
}