{
    "manifestVersion": 1,
    "id": "LoadRunnerProfessionalTestsAutomation",
    "name": "LoadRunner Professional Tests Automation",
    "version": "0.0.4",
    "publisher": "Micro-Focus",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Execute LoadRunner Professional tests from file system",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/LoadRunner.png"        
    },
    "files": [
        {
            "path": "LoadRunnerTask"
        }
    ],
    "content": {
        "details": {
          "path": "README.md"
        },
        "overview": {
          "path": "overview.md"
        },
        "license": {
          "path": "LICENSE.md"
        }
      },
    "tags": [
        "LoadRunner",
        "LoadRunner Professional",
        "Tests Automation",
        "Continuous Integration"
	],
    "contributions": [
        {
            "id": "LoadRunnerProfessionalTestsAutomation",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "LoadRunnerTask"
            }
        }
    ]
}